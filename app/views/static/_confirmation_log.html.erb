<% recent_audit_items.each do |recent_audit| %>
	<div class='homepage-block col-md-3'>
		<h4>
			<%= recent_audit.user.full_name %>
		</h4>

		<p>
			<span class='pending-details'>Week Starting:</span> <%= recent_audit.start_date %>
		</p>

		<p>
			<span class='pending-details'>Confirmed at:</span> <%= recent_audit.end_date || status_label('pending') %>
		</p>
		<p>
			<span class='pending-details'>Status:</span> <%= status_label recent_audit.status %>
		</p>
	</div>
<% end %>