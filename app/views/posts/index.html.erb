<h1>Posts sdsd</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th>
        #
      </th>
      <th>
        Date
      </th>
      <th>
        Last Name
      </th>
      <th>
        Rationale
      </th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td>
          <%= post.id %>
        </td>
        <td>
          <%= post.date %>
        </td>
        <td>
          <%= post.user.last_name %>
        </td>
        <td>
          <%= post.rationale %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>